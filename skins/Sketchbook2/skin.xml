<skin>
    <name>Sketchbook</name>
    <author>Peter</author>
    <version>1.0</version>
    <description>Full colors~</description>
    <background>
        <png>bg.png</png>
        <animation>
            <enabled>false</enabled>
            <speed>8</speed>
            <framecount>4</framecount>
        </animation>
    </background>
    <character>
        <png>char.png</png>
        <hot><png>char_hot.png</png></hot>
        <x>230</x>
        <y>5</y>
        <flip>false</flip>
		<bobbing>
			<enabled>true</enabled>
            <amplitude>4</amplitude>
            <speed>1</speed>
		</bobbing>
    </character>
    <weather>
        <icon>
            <sunny>
                <png>weather/sunny.png</png>
                <animation>
                    <enabled>true</enabled>
                    <speed>5</speed>
                    <framecount>6</framecount>
                </animation>
            </sunny>
            <cloudy><png>weather/cloudy.png</png></cloudy>
            <rainy><png>weather/rainy.png</png></rainy>
            <snowy><png>weather/snowy.png</png></snowy>
            <thunderstorm><png>weather/thunderstorm.png</png></thunderstorm>
            <foggy>
                <png>weather/foggy.png</png>
                <animation>
                    <enabled>true</enabled>
                    <speed>5</speed>
                    <framecount>6</framecount>
                </animation>
            </foggy>
            <night><png>weather/night.png</png></night>
            <width>110</width>
            <height>110</height>
            <x>20</x>
            <y>115</y>
            <bobbing>
                <enabled>false</enabled>
                <speed>5</speed>
            </bobbing>
        </icon>
        <text>
            <fontindex>0</fontindex>
            <x>138</x>
            <y>129</y>
            <size>67</size>
        </text>
    </weather>
    <hwmon>
        <fontindex>0</fontindex>
        <cpu>
            <usage>
                <text>
                    <x>560</x>
                    <y>40</y>
                    <size>48</size>
                </text>
                <icon>
                    <path>icon/cpu.png</path>
                    <x>534</x>
                    <y>68</y>
                    <width>0</width>
                    <height>0</height>
                </icon>
            </usage>
            <temp>
            </temp>
            <combine>True</combine>
            <combinedivider>  @  </combinedivider>
			<pincombinedivider>True</pincombinedivider>
        </cpu>
        <mem>
            <usage>
                <text>
                    <x>560</x>
                    <y>100</y>
                    <size>46</size>
                </text>
                <icon>
                    <path>icon/mem.png</path>
                    <x>528</x>
                    <y>118</y>
                    <width>0</width>
                    <height>0</height>
                </icon>
            </usage>
        </mem>
        <train>
            <next>
				<text>
					<x>560</x>
					<y>166</y>
					<size>40</size>
					<divider>  |  </divider>
				</text>
                <icon>
                    <path>icon/train.png</path>
                    <x>520</x>
                    <y>166</y>
                    <width>0</width>
                    <height>0</height>
                </icon>
            </next> 
        </train>
    </hwmon>
    <fonts>
        <font id="0">
            <ttf>font/OneStrokeScriptBold.ttf</ttf>
            <outline>
                <enabled>true</enabled>
                <color>#444444</color>
                <thickness>3</thickness>
            </outline>
            <color>#FFFFFF</color>
        </font>
        <font id="1"><ttf>font/NationalPark-Outline.ttf</ttf></font>
    </fonts>
    <effects>
        <jpegify>
            <enabled>true</enabled>
            <quality>10</quality>
        </jpegify>
    </effects>
    <flash>
        <background>false</background>
        <character>true</character>
        <weather>false</weather>
        <icons>false</icons>
		<loading>true</loading>
    </flash>
</skin>